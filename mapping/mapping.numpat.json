        "children" : [{
            "name" : "formatFichier",
            "value" : [{
                "method" : "xpath",
                "paths" : [".//mets:FLocat/@xlink:href[contains(., 'file://master/')]", ".//mets:FLocat/@xlink:href[contains(., 'file://ocr/')]"],
                "filters" : ["format"]
            }]
        }, {
            "name" : "nomFichier",
            "value" : [{
                "method" : "xpath",
                "paths" : [".//mets:FLocat/@xlink:href[contains(., 'file://master/')]", ".//mets:FLocat/@xlink:href[contains(., 'file://ocr/')]"],
                "filters" : ["filename"]
            }]
        }, {
            "name" : "empreinteOri",
            "attributes" : {
                "type" : "MD5"
            },
            "value" : [{
                "method" : "xpath",
                "paths" : [".//mets:FLocat/@xlink:href[contains(., 'file://master/')]", ".//mets:FLocat/@xlink:href[contains(., 'file://ocr/')]"],
                "filters" : ["md5"]
            }]
        }]
    }]
