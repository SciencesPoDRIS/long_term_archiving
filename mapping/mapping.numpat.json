                "paths" : [".//mods:namePart"],
                "filters" : ["concat"]
            }],
            "default_values" : ["Non renseigné"]
        }, {
            "name" : "subject",
            "attributes" : {
                "language" : "fra"
            },
            "value" : [{
                "method" : "xpath",
