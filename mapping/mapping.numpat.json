                "paths" : [".//mods:identifier[@type=\"callnumber\"]"]
            }]
        }, {
            "name" : "language",
            "default_values" : ["fra"]
        }, {
            "name" : "coverage",
            "attributes" : {
                "language" : "fra"
            },
            "value" : [{
                "method" : "xpath",
                "paths" : [".//mods:geographic", ".//mods:temporal"],
                "filters" : ["all"]
            }, {
                "method" : "srusrw",
                "paths" : [".//ns1:datafield[@tag=\"607\"]/ns1:subfield[@code=\"a\"]", ".//ns1:datafield[@tag=\"607\"]/ns1:subfield[@code=\"z\"]"],
                "filters" : ["all"]
            }]
