                "method" : "srusrw",
                "paths" : [".//ns1:datafield[@tag=\"606\"]/ns1:subfield[@code=\"a\"]", ".//ns1:datafield[@tag=\"606\"]/ns1:subfield[@code=\"y\"]", ".//ns1:datafield[@tag=\"606\"]/ns1:subfield[@code=\"x\"]", ".//ns1:datafield[@tag=\"607\"]/ns1:subfield[@code=\"x\"]"],
                "filters" : ["all"]
